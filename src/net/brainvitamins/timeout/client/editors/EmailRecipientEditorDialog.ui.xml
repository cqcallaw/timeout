<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:net.brainvitamins.timeout.client.editors">
	<g:VerticalPanel horizontalAlignment="ALIGN_LEFT">
		<e:EmailRecipientEditor ui:field="editor" />
		<g:Cell horizontalAlignment="ALIGN_RIGHT">
			<g:HorizontalPanel horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_BOTTOM">
				<g:Button ui:field="okButton" text="OK" />
				<g:Button ui:field="cancelButton" text="Cancel" />
			</g:HorizontalPanel>
		</g:Cell>
		<g:Label ui:field="errorLabel" width="215px" styleName="error-Label"/>
	</g:VerticalPanel>
</ui:UiBinder>