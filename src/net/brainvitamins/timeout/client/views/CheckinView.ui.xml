<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:HTMLPanel>
		<div id="checkin">
			<g:Button ui:field="checkinButton" text="Checkin" />
			for
			<input type="text" name="timeout" ui:field="timeoutField" value="1000" />
			milliseconds.
		</div>
	</g:HTMLPanel>
</ui:UiBinder>