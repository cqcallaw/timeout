<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:net.brainvitamins.vigilance.client.editors">
	<g:VerticalPanel horizontalAlignment="ALIGN_LEFT"
		styleName="dialog">
		<e:EmailRecipientEditor ui:field="editor" />
		<g:Cell horizontalAlignment="ALIGN_RIGHT">
			<g:HorizontalPanel horizontalAlignment="ALIGN_RIGHT"
				verticalAlignment="ALIGN_BOTTOM">
				<g:Button ui:field="okButton" text="OK" styleName="recipient-Button" />
				<g:Button ui:field="cancelButton" text="Cancel" styleName="recipient-Button" />
			</g:HorizontalPanel>
		</g:Cell>
		<g:Label ui:field="errorLabel" width="215px" styleName="error-Label"
			visible="false" />
	</g:VerticalPanel>
</ui:UiBinder>