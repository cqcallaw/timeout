<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">

	<g:HTMLPanel>
		<c:CellTable ui:field="activityView" />

		<div id="checkin">
			<g:Button ui:field="checkinButton" text="Checkin" tabIndex="1" focus="true" />
			for
			<g:TextBox name="timeout" ui:field="timeoutField" tabIndex="2"
				value="1" />
			hour(s)

			<g:Button ui:field="cancelCheckinButton" text="Cancel Checkin" tabIndex="3"/>
			<g:Label ui:field="errorLabel" styleName="error-Label" visible="false" />
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 